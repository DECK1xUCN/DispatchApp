generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin", "darwin-arm64"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Flight {
  id    Int    @id @default(autoincrement())
  sites Site[]
}

model Site {
  id      Int      @id @default(autoincrement())
  flights Flight[]
}
